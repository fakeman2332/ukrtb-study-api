{
  "name": "ukrtb-study-api",
  "version": "1.1.1",
  "type": "module",
  "description": "JavaScript SDK for study.ukrtb.ru API",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "eslint ./src && tsup",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "vitest run"
  },
  "keywords": [
    "api",
    "ukrtb",
    "sdk",
    "typescript",
    "javascript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fakeman2332/ukrtb-study-api.git"
  },
  "bugs": {
    "url": "https://github.com/fakeman2332/ukrtb-study-api/issues"
  },
  "homepage": "https://github.com/fakeman2332/ukrtb-study-api",
  "author": "Damir Bulatov (admin@fakeman.xyz)",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "eslint": "^9.29.0",
    "prettier": "^3.6.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "tsup": "^8.5.0"
  }
}
